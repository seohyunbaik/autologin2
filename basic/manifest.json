{
  "manifest_version": 3,
  "name": "AutoLogin Extension",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "content.js"
  },
  "action": {
    "default_popup": "",
    "default_icon": {}
  },
  "content_scripts": [{
    "matches": ["https://iam2.kaist.ac.kr/*"],
    "js": ["content.js"]
  }]
}
